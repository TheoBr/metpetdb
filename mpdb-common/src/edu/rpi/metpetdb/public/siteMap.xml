<!-- Used to create a tree representing the hierarchy of the website. -->
<!-- <name> is the name we want to give the page. -->
<!-- If the name needs to be dynamic (V6-B instead of 'Sample Details') 
	 then <screen>false<screen> is added and Specific code will need to be added to Breadcrumbs.java. -->
<!-- <token> is the name the page is given in the TokenSpace class, 
	 these should all be strings found in LocaleEntity.properties -->
	  
<?xml version="1.0" encoding="US-ASCII"?>
<SiteMap>
<page>
	<name>Home</name>
	<token>Introduction</token>
	<page>
		<name>All My Samples</name>
		<token>SamplesForUser</token>
		<page>
			<name>Enter Sample</name>
			<token>UploadSample</token>
		</page>
		<page>
			<name>Bulk Upload</name>
			<token>BulkUpload</token>
		</page>
		<page>
			<name>Specific Sample</name>
			<token>SampleDetails</token>
			<screen>false</screen>
			<page>
				<name>Enter Subsample</name>
				<token>UploadSubsample</token>
				<screen>false</screen>
			</page>
			<page>
				<name>Specific Subsample</name>
				<token>SubsampleDetails</token>
				<screen>false</screen>
				<page>
					<name>Map</name>
					<token>ImageBrowserDetails</token>
					<screen>false</screen>
				</page>
				<page>
					<name>Create Map</name>
					<token>CreateImageMap</token>
					<screen>false</screen>
				</page>
				<page>
					<name>Images</name>
					<token>ImageListViewer</token>
				</page>
				<page>
					<name>Specific Analysis</name>
					<token>ChemicalAnalysisDetails</token>
					<screen>false</screen>
				</page>
				<page>
					<name>Enter Chemical Analysis</name>
					<token>UploadChemicalAnalysis</token>
					<screen>false</screen>
				</page>
			</page>
		</page>
	</page>
	<page>
		<name>My Projects</name>
		<token>AllProjects</token>
		<page>
			<name>New Project</name>
			<token>NewProject</token>
		</page>
	</page>
	<page>
		<name>Search</name>
		<token>Search</token>
		<page>
			<name>All Public Samples</name>
			<token>AllPublicSamples</token>
		</page>
	</page>
	<page>
		<name>Register</name>
		<token>Register</token>
	</page>
	<page>
		<name>Specific User</name>
		<token>UserDetails</token>
		<screen>false</screen>
		<leftside>UserInfo</leftside>
	</page>
</page>
</SiteMap>

