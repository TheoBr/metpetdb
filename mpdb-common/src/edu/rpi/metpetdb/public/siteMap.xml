<!-- Used to create a tree representing the hierarchy of the website. -->
<!-- <name> is the name we want to give the page. -->
<!-- If the name needs to be dynamic (V6-B instead of 'Sample Details') 
	 then <screen>false<screen> is added and Specific code will need to be added to Breadcrumbs.java. -->
<!-- <token> is the name the page is given in the TokenSpace class, 
	 these should all be strings found in LocaleEntity.properties -->
<!-- <leftside> is the name of the left side bar we want for the page. They need to match the Locale as well -->
	  
<?xml version="1.0" encoding="US-ASCII"?>
<SiteMap>
<page>
	<name>Home</name>
	<token>home</token>
	<leftside>UserInfo</leftside>
	<page>
		<name>My Samples</name>
		<token>my-samples</token>
		<leftside>MySamples</leftside>
		<page>
			<name>Add Sample</name>
			<token>sample-add</token>
			<leftside>MySamples</leftside>
		</page>
		<page>
			<name>Bulk Upload</name>
			<token>bulk-upload</token>
			<leftside>MySamples</leftside>
		</page>
		<page>
			<name>Specific Sample</name>
			<token>sample</token>
			<screen>false</screen>
			<leftside>MySubsamples</leftside>
			<page>
				<name>Add Subsample</name>
				<token>subsample-add</token>
				<screen>false</screen>
				<leftside>MySubsamples</leftside>
			</page>
			<page>
				<name>Specific Subsample</name>
				<token>subsample-edit</token>
				<screen>false</screen>
				<leftside>MySubsamples</leftside>
			</page>
			<page>
				<name>Specific Subsample</name>
				<token>subsample</token>
				<screen>false</screen>
				<leftside>MySubsamples</leftside>
				<page>
					<name>Image Map</name>
					<token>ImageBrowserDetails</token>
					<screen>false</screen>
					<leftside>LeftSideLayer</leftside>
				</page>
				<page>
					<name>Create Map</name>
					<token>CreateImageMap</token>
					<screen>false</screen>
					<leftside>LeftSideLayer</leftside>
				</page>
				<page>
					<name>Images</name>
					<token>ImageListViewer</token>
					<leftside>MySubsamples</leftside>
				</page>
				<page>
					<name>Specific Analysis</name>
					<token>ChemicalAnalysisDetails</token>
					<screen>false</screen>
					<leftside>MySubsamples</leftside>
				</page>
				<page>
					<name>Add Chemical Analysis</name>
					<token>chem-analysis-add</token>
					<screen>false</screen>
					<leftside>MySubsamples</leftside>
				</page>
			</page>
		</page>
	</page>
	<page>
		<name>My Projects</name>
		<token>my-projects</token>
		<leftside>MyProjects</leftside>
		<page>
			<name>New Project</name>
			<token>project-new</token>
			<leftside>MyProjects</leftside>
		</page>
		<page>
			<name>Specific Project</name>
			<token>SamplesForProject</token>
			<screen>false</screen>
			<leftside>MyProjects</leftside>
		</page>
	</page>
	<page>
		<name>Search</name>
		<token>search</token>
		<leftside>MySearch</leftside>
		<page>
			<name>All Public Samples</name>
			<token>AllPublicSamples</token>
			<leftside>MySearch</leftside>
		</page>
	</page>
	<page>
		<name>Register</name>
		<token>register</token>
		<leftside>UserInfo</leftside>
	</page>
	<page>
		<name>Specific User</name>
		<token>user</token>
		<screen>false</screen>
		<leftside>UserInfo</leftside>
	</page>
</page>
</SiteMap>

