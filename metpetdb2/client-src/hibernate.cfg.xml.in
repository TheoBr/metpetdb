<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<!-- Generated by MyEclipse Hibernate Tools.                   -->
<hibernate-configuration>

	<session-factory>
		<property name="dialect">
			org.hibernate.dialect.PostgreSQLDialect
		</property>
		<property name="connection.url">
			jdbc:postgresql://@DATABASE_HOST@/@DATABASE@
		</property>
		<property name="connection.username">@APPUSER@</property>
		<property name="connection.password">@APPPASS@</property>
	
		<property name="connection.driver_class">
			org.postgresql.Driver
		</property>
		<property name="myeclipse.connection.profile">
			mpdb-dev
		</property>
		<mapping class="User" />
		<mapping
			class="edu.rpi.metpetdb.server.model.ChemicalAnalysisElements" />
		<mapping
			class="edu.rpi.metpetdb.server.model.ChemicalAnalysisOxides" />
		<mapping class="edu.rpi.metpetdb.server.model.GeometryColumns" />

	</session-factory>

</hibernate-configuration>